                "&:first-child": {
                  "padding-right": "30px"
                },
                "&:last-child": {
                  "padding-left": "30px"
                }
              }
            },
            "H2": {
              "color": "#ea5515"
            },
            ".mbr-text B": {
              "color": "#0f7699"
            },
            "I": {
              "color": "#ea5515"
            },
            "B": {
              "color": "#dc5217"
            },
            "H5": {
              "text-align": "left"
            }
